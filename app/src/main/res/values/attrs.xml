<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomEditText">
        <attr name="labelText" format="string" />
        <attr name="hintText" format="string" />
        <attr name="isPasswordTypeInput" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CustomItemProfile">
        <attr name="addImg" format="reference" />
        <attr name="addText" format="string" />
        <attr name="dividerVisibility" format="enum">
            <enum name="visible" value="0" />
            <enum name="invisible" value="1" />
            <enum name="gone" value="2" />
        </attr>
        <attr name="addTextColor" format="color" />
    </declare-styleable>
</resources>